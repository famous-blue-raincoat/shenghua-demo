        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintVertical_bias="0.26999998" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
